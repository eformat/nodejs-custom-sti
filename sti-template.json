{
   "kind": "List",
   "apiVersion": "v1",
   "metadata": {
      "name": "nodejs-custom-sti"
   },
   "items": [
      {
         "kind": "BuildConfig",
         "apiVersion": "v1",
         "metadata": {
            "name": "nodejs-custom-sti"
         },
         "spec": {
            "triggers": [
               {
                  "type": "GitHub",
                  "github": {
                     "secret": "secret"
                  }
               },
               {
                  "type": "Generic",
                  "generic": {
                     "secret": "secret"
                  }
               },
               {
                  "type": "ImageChange",
                  "imageChange": {}
               }
            ],
            "source": {
               "type": "Git",
               "git": {
                  "uri": "https://github.com/eformat/nodejs-custom-sti.git",
                  "ref": "master"
               },
               "contextDir": "/"
            },
            "strategy": {
               "type": "Docker",
               "dockerStrategy": {
                  "from": {
                     "kind": "ImageStreamTag",
                     "name": "rhel72:latest"
                  }
               }
            },
            "output": {
               "to": {
                  "kind": "ImageStreamTag",
                  "name": "nodejs-custom-sti:latest"
               }
            },
            "resources": {}
         }
      },
      {
         "kind": "ImageStream",
         "apiVersion": "v1",
         "metadata": {
            "name": "rhel72"
         },
         "spec": {
            "dockerImageRepository": "registry.access.redhat.com/rhel7.2:latest"
         }
      },
      {
         "kind": "ImageStream",
         "apiVersion": "v1",
         "metadata": {
            "name": "nodejs-custom-sti"
         },
         "spec": {
            "dockerImageRepository": "",
            "tags": [
               {
                  "name": "latest",
                  "annotations": {
                     "description": "Node.JS 5.X STI image",
                     "iconClass": "icon-nodejs",
                     "tags": "builder,nodejs",
                     "supports": "nodejs:5",
                     "version": "1.0"
                  }
               }
            ]
         }
      }
   ]
}
